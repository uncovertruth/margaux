    "es6-promisify": "^5.0.0",
    "express": "^4.14.1",
    "inliner": "^1.9.5",
    "lodash": "^4.16.4",
    "mkdirp": "^0.5.1",
    "moment": "^2.15.2",
    "morgan": "^1.8.1",
    "portastic": "^1.0.1",
    "raven": "^1.1.2",
    "uuid": "^3.0.1",
    "validator": "^6.1.0"
  },
  "devDependencies": {
    "@uncovertruth/eslint-config-flowtype": "^0.1.0",
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
